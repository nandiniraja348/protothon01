import sympy
import numpy as np 
x,y= sympy.symbols("x y")
y=1
c=[]
a=int(input("Enter the rows "))
for i in range (1,a+1):
    formula = (x + y) ** i
    print(formula.expand())
    c.append(formula.expand())
print(c)    
length=len(c)    
for i in range(0,length):
    a=np.poly(c[i])
    print(a.np.poly1d.coeffs())
    
